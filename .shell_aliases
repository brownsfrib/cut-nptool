alias l='ls'
alias la='ls -a'
alias ls="ls -F --color"
alias ldir="ls -lF | grep /"
alias ll='ls -l'
alias lll='ls -al'
alias m='more'
alias cp='cp -i'
alias mv='mv -i'
alias rm='rm -i'
alias dir='ll'

alias ..='cd ..'
alias ,,='cd -'

# Creating tree alias
alias tree="find . -print | sed -e 's;[^/]*/;|____;g;s;____|; |;g'"

